{
    "name": "react-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd": "^3.25.3",
        "connected-react-router": "^6.6.0",
        "i18next": "^19.0.1",
        "i18next-browser-languagedetector": "^4.0.1",
        "jwt-decode": "^2.2.0",
        "moment": "^2.24.0",
        "node-sass": "^7.0.0",
        "prettier": "^2.2.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-i18next": "^11.2.2",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0",
        "redux": "^4.0.4",
        "redux-actions": "^2.6.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.1.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-prettier": "^3.3.0",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,md,html}": [
            "prettier --write"
        ]
    }
}
